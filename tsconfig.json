{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["resources/js/*"]
    },
    "allowJs": true,
    "noEmit": true
  },
  "include": ["resources/js/**/*"],
  "exclude": ["node_modules", "public"]
}
